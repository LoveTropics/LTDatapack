[
  {
    "type": "ltminigames:set_game_types",
    "all": "adventure"
  },
  {
    "type": "ltminigames:position_players",
    "all": ["$regions.spawn"]
  },
  "ltminigames:disable_hunger",
  {
    "type": "ltminigames:set_game_rules",
    "rules": {
      "doDaylightCycle": "false",
      "doWeatherCycle": "false",
      "doMobSpawning": "false",
      "fallDamage": "false",
      "announceAdvancements": "false"
    }
  },
  "ltminigames:immediate_respawn",
  {
    "type": "ltminigames:cancel_player_damage",
    "knockback": true
  },
  {
    "type": "ltminigames:timed",
    "length": 2400,
    "countdown_seconds": 3,
    "timer_bar": {
      "text": "Game starting in %1$s: ready-up!",
      "color": "green"
    }
  },
  {
    "type": "ltminigames:ready_up"
  }
]
