{
  "type": "ltminigames:apply_for_time",
  "apply": {
    "type": "ltminigames:add_attribute_modifier",
    "attribute": "$attribute.type",
    "modifier": {
      "id": "$attribute.id",
      "name": "$attribute.name",
      "amount": "$attribute.amount",
      "operation": "$attribute.operation"
    }
  },
  "clear": {
    "type": "ltminigames:clear_attribute_modifier",
    "id": "$attribute.id",
    "attribute": "$attribute.type"
  },
  "seconds": "$seconds",
  "indicator": "$indicator"
}